{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "strictNullChecks": true,
    "paths": {
        "*": ["./src/*"]
    }
  },
  "ts-node": {
    "compilerOptions": {
      "module": "CommonJS",
      "jsx": "react-jsx"
    }
  },
  "include": [
    "./config/jest/setupTests.ts",
    "./src/**/*.tsx",
    "./src/**/*.ts",
    "./.eslintrc.js"
, "src/shared/lib/components/DynamicModuleLoader/DynamicModuleLoader.tss"  ]
}